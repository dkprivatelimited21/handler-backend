{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
	"routes":[
	{
		"src":"/(.*)",
		"dest":"src/server.js",
		"methods":["GET","POST","PUT","DELETE","OPTIONS"],
		"headers":{
			"Access-Control-Allow-Origin":"https://local-handler.vercel.app"
			   },
		"credentials": "true"
	}
      ]
}